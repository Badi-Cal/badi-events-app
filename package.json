{
  "name": "@badi-events/app",
  "version": "0.1.0",
  "description": "Events app for Badi calendar",
  "keywords": [
    "events",
    "badi",
    "calendar"
  ],
  "license": "GPL-3.0-or-later",
  "scripts": {
    "clean": "rm -rf dist/ .parcel-cache && rm -rf node_modules/",
    "prestart": "npm run --silent lint",
    "start": "parcel app/index.html",
    "build": "parcel build app/index.html --public-url ./",
    "lint": "eslint --cache --ext .js,.mjs app/",
    "pretest": "npm run --silent lint",
    "test": "npm explore @badi-events/test -- npm run --silent start"
  },
  "engines": {
    "node": ">=14.9.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@fullcalendar/bootstrap": "^5.3.0",
    "@fullcalendar/core": "^5.3.0",
    "@fullcalendar/daygrid": "^5.3.0",
    "@fullcalendar/interaction": "^5.3.0",
    "@fullcalendar/list": "^5.3.0",
    "@fullcalendar/timegrid": "^5.3.0",
    "@popperjs/core": "^2.9.2",
    "backbone": "^1.4.0",
    "backbone.localstorage": "^2.0.2",
    "bootstrap": "^4.6.0",
    "jquery": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@badi-events/test": "file:test",
    "@parcel/config-default": "^2.0.0-nightly.638",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.23.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^8.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "parcel": "^2.0.0-nightly.560",
    "postcss": "^8.2.9",
    "postcss-nested": "^5.0.5",
    "postcss-nested-ancestors": "^2.0.0",
    "sass": "^1.32.8"
  }
}
